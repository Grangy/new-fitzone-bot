{
  "name": "new-fitzone-bot",
  "version": "1.0.0",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node index.js",
    "prestart": "npm run build",
    "build": "npm run prisma:generate && npm run prisma:deploy && cp -r dist/* . && cp -r src/generated .",
    "postbuild": "npm run prisma:generate",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:reset": "npx prisma migrate reset --force",
    "import:workouts": "tsx src/scripts/import-workouts.ts",
    "setup": "npm run build && npm run prisma:deploy && npm run import:workouts",
    "health": "node -e \"console.log('OK')\"",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@types/node": "^24.9.1",
    "dotenv": "^17.2.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.18.0",
    "telegraf": "^4.16.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {}
}
